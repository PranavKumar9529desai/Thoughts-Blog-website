{
    "version": 2,
    "builds": [
      {
        "src": "common/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1"
      }
    ]
  }